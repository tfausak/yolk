{
  "//": "https://code.visualstudio.com/api/references/extension-manifest",
  "activationEvents": [
    "onLanguage:haskell"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "grammars": [
      {
        "language": "cabal",
        "path": "grammars/cabal-grammar.json",
        "scopeName": "source.cabal"
      },
      {
        "language": "haskell",
        "path": "grammars/haskell-grammar.json",
        "scopeName": "source.haskell"
      }
    ],
    "languages": [
      {
        "aliases": [
          "Cabal"
        ],
        "configuration": "languages/cabal-language-configuration.json",
        "extensions": [
          ".cabal"
        ],
        "id": "cabal"
      },
      {
        "aliases": [
          "Haskell"
        ],
        "configuration": "languages/haskell-language-configuration.json",
        "extensions": [
          ".hs"
        ],
        "id": "haskell"
      }
    ],
    "snippets": [
      {
        "language": "haskell",
        "path": "snippets/haskell-snippets.json"
      }
    ]
  },
  "description": "A Haskell IDE.",
  "displayName": "Yolk",
  "engines": {
    "vscode": "^1.40.0"
  },
  "galleryBanner": {
    "color": "#453A62",
    "theme": "dark"
  },
  "homepage": "https://github.com/tfausak/yolk",
  "icon": "images/yolk.png",
  "keywords": [
    "haskell",
    "ide"
  ],
  "main": "client/index.js",
  "name": "yolk",
  "publisher": "taylorfausak",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfausak/yolk.git"
  },
  "version": "0.0.0"
}
